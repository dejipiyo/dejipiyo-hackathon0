{
  "name": "rebuild-base-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint -d ./ --ignore-path .gitignore",
    "lint:fix": "next lint -d ./ --ignore-path .gitignore --fix .",
    "type:check": "tsc --pretty --noEmit",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "test": "jest",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@hookform/resolvers": "^2.9.11",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "libphonenumber-js": "^1.10.21",
    "next": "^13.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "typescript": "4.9.5",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/client-preset": "2.1.0",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-msw": "^1.1.6",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "msw": "^1.1.0",
    "prettier": "^2.8.4",
    "react-test-renderer": "^18.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
